{
  "name": "programmer-hq",
  "displayName": "Programmer hq",
  "version": "0.0.1",
  "description": "A dashboard for programmers",
  "author": "Giliwawa",
  "scripts": {
    "dev": "plasmo dev",
    "build": "plasmo build",
    "package": "plasmo package"
  },
  "dependencies": {
    "@clerk/clerk-react": "^4.15.3",
    "@supabase/auth-helpers-react": "^0.3.1",
    "@supabase/supabase-js": "^2.21.0",
    "plasmo": "0.67.1",
    "react": "18.2.0",
    "react-dom": "18.2.0"
  },
  "devDependencies": {
    "@plasmohq/prettier-plugin-sort-imports": "3.6.3",
    "@tailwindcss/typography": "^0.5.9",
    "@types/chrome": "0.0.210",
    "@types/node": "18.11.18",
    "@types/react": "18.0.27",
    "@types/react-dom": "18.0.10",
    "autoprefixer": "^10.4.14",
    "postcss": "^8.4.21",
    "prettier": "2.8.3",
    "tailwindcss": "^3.3.1",
    "typescript": "4.9.4"
  },
  "manifest": {
    "host_permissions": [
      "https://*/*"
    ],
    "key": "$CRX_KEY",
    "web_accessible_resources": [
      {
        "resources": [
          "options.html"
        ],
        "matches": [
          "<all_urls>"
        ],
        "extension_ids": [
          "$CRX_ID"
        ]
      }
    ]
  }
}
